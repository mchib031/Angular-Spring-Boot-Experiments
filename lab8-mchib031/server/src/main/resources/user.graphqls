type Query{
    users:[User]
    userById(userId:ID!):User
}
type User{
    userId:ID!
    firstName:String!
    lastName:String!
    phoneNumber:String
    email: String
}

type Mutation{
    newUser(
        firstName:String!
        lastName:String!
        phoneNumber:String
        email:String
    ):User!
    deleteUser(userId:ID!):Boolean
    updateUser(userId:ID!,
        firstName:String
        lastName:String
        phoneNumber:String
        email:String
    ):User!
}